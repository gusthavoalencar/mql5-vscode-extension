{
  "name": "mql5-vscode-extension",
  "displayName": "MQL5 Custom Syntax",
  "description": "Syntax highlighting and language support for MQL5 in VS Code.",
  "version": "0.1.0",
  "publisher": "gusthavoalencar",
  "repository": {
    "type": "git",
    "url": "https://github.com/gusthavoalencar/mql5-vscode-extension.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mql5",
        "aliases": [
          "MQL5",
          "mql5"
        ],
        "extensions": [
          ".mq5",
          ".mqh"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mql5",
        "scopeName": "source.mql5",
        "path": "./syntaxes/mql5.tmLanguage.json"
      }
    ]
  },
  "icon": "icon.png",
  "keywords": [
    "mql5",
    "metatrader",
    "trading",
    "syntax",
    "language",
    "highlighting"
  ],
  "bugs": {
    "url": "https://github.com/gusthavoalencar/mql5-vscode-extension/issues"
  },
  "homepage": "https://github.com/gusthavoalencar/mql5-vscode-extension#readme",
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {}
}
